@{
	Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Document</title>

	<style>
		.main {
			padding: 100px;
		}
	</style>
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
</head>
<body>

	<!-- nav start here -->
	<nav class="navbar navbar-light bg-light">
		<div class="container-fluid">
			<a class="navbar-brand">Customar Pannal</a>
			<button class="btn btn-outline-danger me-2" id="logoutBtn" type="submit">LogOut</button>
			<form class="d-flex">
				<input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
				<button class="btn btn-outline-success" type="submit">Search</button>
			</form>
		</div>
	</nav>
	<!-- nav end here -->






	<div class="main">
		<div id="fff23"></div>
		<!-- Accordion start here -->
		<div class="accordion accordion-flush" id="accordionFlushExample">
			<div class="accordion-item">
				<h2 class="accordion-header" id="flush-headingOne">
					<button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseOne" aria-expanded="false" aria-controls="flush-collapseOne">
						Subscribe Service
					</button>
				</h2>
				<div id="flush-collapseOne" class="accordion-collapse collapse" aria-labelledby="flush-headingOne" data-bs-parent="#accordionFlushExample">
					<div class="accordion-body">
						<!-- buying service form start here -->
						<form class="row g-3">
							<div class="col-md-6">
								<label for="inputEmail4" class="form-label">Name</label>
								<input type="text" class="form-control" id="inputEmail4">
							</div>
							<div class="col-md-6">
								<label for="inputPassword4" class="form-label">Email</label>
								<input type="email" class="form-control" id="inputPassword4">
							</div>
							<div class="col-md-6">
								<label for="inputPassword4" class="form-label">Phone</label>
								<input type="text" class="form-control" id="inputPassword4">
							</div>
							<div class="col-6">
								<label for="inputAddress" class="form-label">Address</label>
								<input type="text" class="form-control" id="inputAddress" placeholder="1234 Main St">
							</div>
							<div class="col-12">
								<label for="inputAddress2" class="form-label">Detail Address</label>
								<input type="text" class="form-control" id="inputAddress2" placeholder="Apartment, studio, or floor">
							</div>
							<div class="col-md-6">
								<label for="inputCity" class="form-label">City</label>
								<input type="text" class="form-control" id="inputCity">
							</div>
							<div class="col-md-4">
								<label for="inputState" class="form-label">Service Name</label>
								<select id="ServiceId" class="form-select">
									<option>Select</option>
									<option value="1">Brodbank Connection</option>
									<option value="2">Land line Connection</option>
									<option value="3">Dial-Up Connection</option>
								</select>
							</div>
							<div class="col-md-2">
								<label for="inputZip" class="form-label">Zip</label>
								<input type="text" class="form-control" id="inputZip">
							</div>
							@* Dail up connection *@
							<div class="col-12" style="display:none;border:1px solid gray;padding:12px;" id="DailUp">
								<h3>Dial-Up Connection</h3>
								<h6>Hourly Basis</h6>
								<div>
									<label>
										<input type="radio" name="ccc" value="10Hours">
										10 Hrs. – $50 (validity is for one Month)
									</label>
								</div>
								<div>
									<label>
										<input type="radio" name="ccc" value="30Hours">
										30 Hrs. – $130 (validity is for 3 Months)
									</label>
								</div>
								<div>
									<label>
										<input type="radio" name="ccc" value="60Hours">
										60 Hrs. – $260 (validity is for 6 Months)
									</label>
								</div>

								<h6>Unlimited 28Kbps</h6>
								<div>
									<label>
										<input type="radio" name="ccc" value="28kbpsMonthly">
										Monthly – $75
									</label>
								</div>
								<div>
									<label>
										<input type="radio" name="ccc" value="28kbpsQuarterly">
										Quarterly – $150
									</label>
								</div>

								<h6>Unlimited 56 Kbps</h6>
								<div>
									<label>
										<input type="radio" name="ccc" value="56kbpsMonthly">
										Monthly – $100
									</label>
								</div>
								<div>
									<label>
										<input type="radio" name="ccc" value="56kbpsQuarterly">
										Quarterly – $180
									</label>
								</div>
							</div>
							@* end dail up connection *@
							@* Broadband Connection *@
							<div class="col-12" style="display:none;border:1px solid gray;padding:12px;" id="BroadbandConnection">
								<h3>Broadband Connection</h3>

								<h6>Hourly Basis</h6>
								<div>
									<label>
										<input type="radio" name="ccc" value="30Hours">
										30 Hrs. – $175 (validity is for 1 Month)
									</label>
								</div>
								<div>
									<label>
										<input type="radio" name="ccc" value="60Hours">
										60 Hrs. – $315 (validity is for 6 Months)
									</label>
								</div>

								<h6>Unlimited 64Kbps</h6>
								<div>
									<label>
										<input type="radio" name="ccc" value="64kbpsMonthly">
										Monthly – $225
									</label>
								</div>
								<div>
									<label>
										<input type="radio" name="ccc" value="64kbpsQuarterly">
										Quarterly – $400
									</label>
								</div>

								<h6>Unlimited 128 Kbps</h6>
								<div>
									<label>
										<input type="radio" name="ccc" value="128kbpsMonthly">
										Monthly – $350
									</label>
								</div>
								<div>
									<label>
										<input type="radio" name="ccc" value="128kbpsQuarterly">
										Quarterly – $445
									</label>
								</div>
							</div>
							@* brod band connection end here *@
							@* Land Line Connection *@
							<div class="col-12" style="display:none;border:1px solid gray;padding:12px;" id="LandLineConnection">
								<h3>Land Line Connection</h3>

								<h6>Local Plan</h6>
								<div>
									<label>
										<input type="radio" name="ccc" value="Unlimited">
										Unlimited - $75 (Valid for one year, rental only)
									</label>
								</div>
								<p>The call charges are: 55 cents / minute</p>
								<div>
									<label>
										<input type="radio" name="ccc" value="Monthly">
										Monthly Plan - $35 (Valid for one month, rental only)
									</label>
								</div>
								<p>The call charges are: 75 cents / minute</p>

								<h6>STD Plan</h6>
								<div>
									<label>
										<input type="radio" name="ccc" value="Monthly">
										Monthly - $125 (Valid for one month, rental only)
									</label>
								</div>
								<p>The call charges are:</p>
								<ul>
									<li>Local: 70 cents / minute</li>
									<li>STD: $2.25 / minute</li>
									<li>Messaging For Mobiles: $1.00 / Minute</li>
								</ul>

								<div>
									<label>
										<input type="radio" name="ccc" value="HalfYearly">
										Half-Yearly - $420 (Valid for six months, rental only)
									</label>
								</div>
								<p>The call charges are:</p>
								<ul>
									<li>Local: 60 cents / minute</li>
									<li>STD: $2.00 / minute</li>
									<li>Messaging For Mobiles: $1.15 / Minute</li>
								</ul>

								<div>
									<label>
										<input type="radio" name="ccc" value="Yearly">
										Yearly - $700 (Valid for one year, rental only)
									</label>
								</div>
								<p>The call charges are:</p>
								<ul>
									<li>Local: 60 cents / minute</li>
									<li>STD: $1.75 / minute</li>
									<li>Messaging For Mobiles: $1.25 / Minute</li>
								</ul>
							</div>

							<div class="col-12">
								<button type="submit" id="RegisterServices" class="btn btn-primary">Buy Now</button>
							</div>
						</form>
						<!-- buying service form end here -->
					</div>
				</div>
			</div>
			<div class="accordion-item">
				<h2 class="accordion-header" id="flush-headingTwo">
					<button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseTwo" aria-expanded="false" aria-controls="flush-collapseTwo">
						Payment History
					</button>
				</h2>
				<div id="flush-collapseTwo" class="accordion-collapse collapse" aria-labelledby="flush-headingTwo" data-bs-parent="#accordionFlushExample">
					<div class="accordion-body">
						<!-- Payment Table is start here -->
						<table class="table">
							<thead>
								<tr>
									<th scope="col">#</th>
									<th scope="col">Service Name</th>
									<th scope="col">Date</th>
									<th scope="col">Cash</th>
									<th scope="col">Services Done</th>
								</tr>
							</thead>
							<tbody>
								<tr>
									<th scope="row">1</th>
									<td>Mark</td>
									<td>Otto</td>
									<td>mdo</td>
									<td>mdo</td>
								</tr>
								<tr>
									<th scope="row">2</th>
									<td>Jacob</td>
									<td>Thornton</td>
									<td>fat</td>
									<td>fat</td>
								</tr>
								<tr>
									<th scope="row">3</th>
									<td colspan="2">Larry the Bird</td>
									<td>twitter</td>
									<td>twitter</td>
								</tr>
							</tbody>
						</table>
						<!-- Payment Table is end here -->
					</div>
				</div>
			</div>
			<div class="accordion-item">
				<h2 class="accordion-header" id="flush-headingFive">
					<button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseFive" aria-expanded="false" aria-controls="flush-collapseFive">
						Monthly Pay
					</button>
				</h2>
				<div id="flush-collapseFive" class="accordion-collapse collapse" aria-labelledby="flush-headingFive" data-bs-parent="#accordionFlushExample">
					<div class="accordion-body">
						<!-- paying form start here  -->
						<form id="serviceForm" class="row gx-3 gy-2 align-items-center">
							<div class="col-sm-3">
								<label class="visually-hidden" for="specificSizeInputName">Name</label>
								<input type="text" class="form-control" id="specificSizeInputName" placeholder="Jane Doe">
							</div>
							<div class="col-sm-3">
								<label class="visually-hidden" for="specificSizeInputGroupUsername">Username</label>
								<div class="input-group">
									<div class="input-group-text"></div>
									<input type="text" class="form-control" id="specificSizeInputGroupUsername" placeholder="Username">
								</div>
							</div>
							<div class="col-sm-3">
								<label class="visually-hidden" for="specificSizeSelect">Preference</label>
								<select class="form-select" id="specificSizeSelect">
									<option selected>Choose...</option>
									<option value="1">One</option>
									<option value="2">Two</option>
									<option value="3">Three</option>
								</select>
							</div>
							<div class="col-sm-3">
								<label for="specificSizeInputName">Amount Of Payment</label>
								<input type="text" class="form-control" id="specificSizeInputName" placeholder="$0.00">
							</div>
							<div class="col-auto">
								<button type="button" id="submitButton" class="btn btn-primary">Submit</button>
							</div>
						</form>

						<!-- Paying method end here  -->
					</div>
				</div>
			</div>

			<div class="accordion-item">
				<h2 class="accordion-header" id="flush-headingThree">
					<button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseThree" aria-expanded="false" aria-controls="flush-collapseThree">
						Own Services
					</button>
				</h2>
				<div id="flush-collapseThree" class="accordion-collapse collapse" aria-labelledby="flush-headingThree" data-bs-parent="#accordionFlushExample">
					<div class="accordion-body">
						<!-- Payment Table is start here -->
						<table class="table">
							<thead>
								<tr>
									<th scope="col">#</th>
									<th scope="col">Service Name</th>
									<th scope="col">Owner Email</th>
									<th scope="col">Duration</th>
									<th scope="col">Service Activation Status</th>
								</tr>
							</thead>
							<tbody>
								<tr>
									<th scope="row">1</th>
									<td>Mark</td>
									<td>Otto</td>
									<td>mdo</td>
									<td>true</td>
								</tr>
								<tr>
									<th scope="row">2</th>
									<td>Jacob</td>
									<td>Thornton</td>
									<td>fat</td>
									<td>true</td>
								</tr>
								<tr>
									<th scope="row">3</th>
									<td colspan="2">Larry the Bird</td>
									<td>twitter</td>
									<td>true</td>
								</tr>
							</tbody>
						</table>
						<!-- Payment Table is end here -->
					</div>
				</div>
			</div>

			<div class="accordion-item">
				<h2 class="accordion-header" id="flush-headingFour">
					<button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseFour" aria-expanded="false" aria-controls="flush-collapseFour">
						Complane
					</button>
				</h2>
				<div id="flush-collapseFour" class="accordion-collapse collapse" aria-labelledby="flush-headingFour" data-bs-parent="#accordionFlushExample">
					<div class="accordion-body">
					</div>
				</div>
			</div>
		</div>

		<!-- Accordion end here -->





	</div>
	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/lib/jquery/dist/jquery.js"></script>
	<script src="~/js/javascript.js"></script>

	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js" integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF" crossorigin="anonymous"></script>
</body>
</html>
